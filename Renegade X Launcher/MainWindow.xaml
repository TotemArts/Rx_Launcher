				AutoReverse="True" 
				/>-->
    </Storyboard>
  </Window.Resources>

  <!-- END RESOURCE -->
  <Border x:Name="WindowInnerBorder" BorderBrush="#000000" BorderThickness="1">
    <Border.Background>
      <ImageBrush ImageSource="Resources/rx_Background.png" Stretch="UniformToFill"/>
    </Border.Background>
    <Grid x:Name="WindowInner">
      <Grid.RowDefinitions>
        <RowDefinition Height="auto"/>
        <RowDefinition Height="*"/>
      </Grid.RowDefinitions>
      <DockPanel Grid.Row="0" x:Name="Header" Background="#EE0B1A22">
