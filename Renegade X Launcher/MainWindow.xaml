                    <Slider Grid.Row="1" Grid.Column="2" HorizontalAlignment="Left" Margin="6" Width="200" x:Name="SD_MaxPlayerSlider" ValueChanged="SD_MaxPlayerSlider_ValueChanged" Maximum="64" SmallChange="1" Value="64"/>
